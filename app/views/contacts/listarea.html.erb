<div class="page-header">
  <h1>List of Areas</h1>
</div>

<%= link_to "Add a new contact", new_contact_path, class: "btn btn-success" %>


<table class="table table-striped table-hover">	
		<% @listarea.each do |area| %>
			<tr class="tablerow">
				<td><%= link_to area, :action => :areacontact, :area => area %></td>
			</tr>
		<% end %>
</table>